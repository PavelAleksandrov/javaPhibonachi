public class ForLoop {

    public static void main(String[] args) {

        int fibNumOne = 0;
        int fibNumTwo = 1;
        int sumFibNum = 1;
        for (int i = sumFibNum; i < 100000; i = fibNumTwo) {
            sumFibNum = fibNumOne + fibNumTwo;
            fibNumOne = fibNumTwo;
            fibNumTwo = sumFibNum;
            System.out.println("Число Фибоначчи " + sumFibNum);
        }
    }
}
